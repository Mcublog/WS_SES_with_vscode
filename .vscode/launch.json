{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Color",
            "type": "python",
            "request": "launch",
            "program": "colorize.py",
            "args": [
                "--command",
                "emBuild -time -config Debug -project ${config:ses_project} ${config:ses_project}.emProject -verbose  "
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug J-Link",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/Output/Debug/Exe/Ws_project.elf",
            // "serverpath": "/opt/SEGGER/JLink/JLinkGDBServerCLExe",
            "serverpath": "C:/Program Files (x86)/SEGGER/JLink/JLinkGDBServerCL.exe",
            "servertype": "jlink",
            "device": "STM32F407VE",
            "interface": "swd",
            "serialNumber": "", //If you have more than one J-Link probe, add the serial number here.
            "runToEntryPoint": "main",
            "svdFile": "${workspaceRoot}/Svd/STM32F407.svd",
            // "debuggerArgs": ["-timeout 5000"]
            // "preLaunchTask": "Build: debug",
            // "showDevDebugOutput":"parsed",
            // "debugServer": 19021
        }
    ]
}